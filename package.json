{
  "name": "@suzulabo/solid-base",
  "version": "0.0.0",
  "description": "Base UI Components of SolidJS",
  "homepage": "https://github.com/suzulabo/solid-base#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/suzulabo/solid-base.git"
  },
  "author": "Kenji Suzuki",
  "keywords": [
    "SolidJS"
  ],
  "license": "Apache-2.0",
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">=16"
  },
  "type": "module",
  "scripts": {
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "prettier": "prettier --check .",
    "prettier:fix": "prettier --write .",
    "test": "vitest run",
    "clean": "pnpm -r run clean",
    "build": "pnpm run --filter !example build && pnpm run --filter example build"
  },
  "devDependencies": {
    "@suzulabo/code-config-typescript": "^0.0.2",
    "@tsconfig/node16-strictest": "^1.0.4",
    "@typescript-eslint/eslint-plugin": "^5.42.0",
    "@typescript-eslint/parser": "^5.42.0",
    "eslint": "^8.26.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-typescript": "^3.5.2",
    "eslint-plugin-import": "^2.26.0",
    "prettier": "^2.7.1",
    "typescript": "^4.8.4"
  }
}
